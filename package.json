{
  "name": "ohmyfood",
  "version": "1.0.0",
  "description": "ohmyfood is a mobile first application where users can book in a restaurant their menu which they was just composed on the application",
  "main": "index.js",
  "scripts": {
    "sass": "sass --watch ./sass/main.scss:./public/css/style.css",
    "normalize": "sass ./sass/vendor/normalize.scss:./public/css/normalize.css --style=compressed",
    "prefix": "postcss ./public/css/style.css --use autoprefixer -d .public/css/prefixed/",
    "sass-min-prefix": "sass ./sass/main.scss:./public/css/style.min.css --style=compressed && postcss ./public/css/style.min.css --use autoprefixer -d ./public/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matanna/ohmyfood.git"
  },
  "author": "Mathieu Bonhommeau",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matanna/ohmyfood/issues"
  },
  "homepage": "https://github.com/matanna/ohmyfood#readme",
  "browserslist": "last 4 versions"
}
